                                                    <div class="item-foot-left">
                                                        <p>
                                                        {% if annonce.user.type == 'simple' %}
                                                        <p><i class="fa fa-user"></i> <a href="{{ path('agent_show',{'id':annonce.user.id}) }}">{{ annonce.user.profileSimpleUser.nom }}</a></p>
                                                        {% elseif annonce.user.type == 'society' %}
